#include<iostream>
using namespace std;
int main()
{
    int i,n,a=0,s=0,b=0;
    cin>>n;
    char *arr = new char[n];
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
        if(arr[i]=='a')
        {
            a++;
        }
        else if(arr[i]=='s')
        {
            s++;
        }
        else if(arr[i]=='p')
        {
            b++;
        }
    }
    cout<<a<<" "<<s<<" "<<b;
    delete []arr;
    
	return 0;
}
